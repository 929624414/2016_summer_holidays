#include<iostream>
#include<cmath>
using namespace std;
class Address
{
    string name,street,city;
    int yb;

public:
    Address(string A,string B,string C,int D)
    {
        name=A;
        street=B;
        city=C;
        yb=D;
    }
    void ChangeName(string A)
    {
        name=A;
    }
    void Display()
    {
        cout<<"name:"<<name<<endl<<"street:"<<street<<endl<<"city:"<<city<<endl<<"yb:"<<yb<<endl;
    }
    ~Address(){}
};
int main()
{
    Address ss("dumplings","idon'tknow","panjin",124000);
    ss.Display();
    ss.ChangeName("dabaozi");
    ss.Display();
    return 0;
}
